package Com.Yasasvi;
import java.util.Scanner;
public class SumEvenOdd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
	    System.out.print("Enter the number of integers: ");	    
	     int n = input.nextInt();
	     int[] numbers = new int[n];
	     int sumEven = 0, sumOdd = 0;
	     System.out.print("Enter " + n + " integers: ");
	     for (int i = 0; i < n; i++) {
	        numbers[i] = input.nextInt();

	     }	    
	     // Calculate the sum of even and odd integers
	     for (int i = 0; i < n; i++)
	     {
	        if (numbers[i] % 2 == 0) {
	           sumEven += numbers[i];
	        } else {
	           sumOdd += numbers[i];

	        }

	     }
	     System.out.println("Sum of even integers: " + sumEven);
	     System.out.println("Sum of odd integers: " + sumOdd);
         
	  }

}



OUTPUT:
Enter the number of integers: 5
Enter 5 integers: 2 3 4 5 6 
Sum of even integers: 12
Sum of odd integers: 8
