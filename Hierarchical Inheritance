package Com.Yasasvi;
class A{
	public void methodA() {
		System.out.println("HELLO");
	}
	public void methodX() {
		System.out.println("HI");
	}
	public void methodY() {
		System.out.println("HEY");
	}
}
class B extends A{
	public void methodB() {
		System.out.println("EVERYONE");
	}
}
class C extends A{
	public void methodC() {
		System.out.println("MY NAME IS");
	}
}
class D extends A{
	public void methodD() {
		System.out.println("XYZ");
	}
}
public class HierarchicalInheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 B obj1 = new B();
	     C obj2 = new C();
	     D obj3 = new D();
	     //All classes can access the method of class A
	     obj1.methodA();
	     obj2.methodX();
	     obj2.methodA();
	     obj3.methodA();
	     obj3.methodY();
	
	}

}

OUTPUT:
HELLO
HI
HELLO
HELLO
HEY
